<!-- clipboard -->
<% if(theme.codeBlock && theme.codeBlock.copy && theme.codeBlock.copy.enable) { %>
  <% if (theme.cdns && theme.cdns.clipboard && theme.cdns.clipboard.enable) { %>
    <script async type="text/javascript" src="<%- theme.cdns.clipboard.url %>"></script>
  <% } else { %>
    <%- js({ src: '/plugins/clipboard.min.js', async: true, type: 'text/javascript' }) %>
  <% } %>
  <%- js(['/js/codeCopy.js']) %>
<% } %>
